import java.util.Scanner;

public class DNA_tester_file{
public static void main(String[] args) throws Exception
{
  Scanner reader = new Scanner(System.in);
  DNA dna = new DNA(); 
  
  dna.userDNAentry();
  dna.stripErrors(); 
  dna.generateRNA();
  dna.searchSequence("AGT");
  dna.simpleSequenceAnalysis();
  dna.searchSubsequence("CAGT");
  dna.replaceSequence("AGT", "CAT"); 
  dna.generateReport(); 
}
}
